<nav class="navbar navbar-default">
  <div class="container-fluid">
    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li class="active"><%= link_to 'LOGOUT', destroy_user_session_path, method: :delete , data: {confirm: 'Are you Sure?'}%></li>
        <li><%= link_to 'SUBMIT QUIZ', '/submit', method: :get , data: {confirm: 'Are you Sure?'}%></li>
        
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>
<%= render 'main' %>
  <div class="row progress">
    <div class="progress-bar progress-bar-striped active" role="progressbar"
      aria-valuenow="<%= @percentage %>" aria-valuemin="0" aria-valuemax="100" style= "width: <%= @percentage %>%">
      <%= @percentage %>% Complete
    </div>
  </div>
<aside>
  <div class="row container-fluid page">
    <nav aria-label="Page navigation">
      <ul class="pagination">
        <% counter=1 %> 
            <% @questions.each do |q| %>
              <li>
                <% str = counter.to_s %>
                <% counter+=1 %>
                <%= link_to str,"/question/#{q.id}" %>
              </li>
            <% end %> 
      </ul>
    </nav>
  </div>
</aside>